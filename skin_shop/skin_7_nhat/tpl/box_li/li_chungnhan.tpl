<div class="item">
    <a href="javascript:void(0)"
        class="brand-item pos-relative d-flex align-items-center aspect-ratio disabled-link" 
        title="{brand_title}"
        style="--width: 304; --height: 170"
        onclick="return false;">
        <img loading="lazy" 
             class="img-fluid m-auto object-contain mh-100 w-auto"
             src="{brand_image}"    
             alt="{brand_title}" 
             width="304" 
             height="170" />
    </a>
</div>

