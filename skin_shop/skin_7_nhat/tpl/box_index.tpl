<div class=" ss_item style2">
    <a href="/san-pham/{cat_blank}.html" title="{cat_tieude}">
        <div class="ss_img">
            <picture>
                <img loading="lazy" class="img-fluid object-contain" src="{cat_img}"
                    style="height: 150px;" alt="{cat_tieude}" />
            </picture>
        </div>
        <div class="ss_info mt-2 text-center">
            <h2 class="heading-bar__title text-dark" style="font-size: 16px;">
                <a href="/san-pham/{cat_blank}.html">{cat_tieude}</a>
            </h2>
            <span class="ss_number text-muted">{total_products} sản phẩm</span>
            <div class="ss_seemore mt-1">
                <svg class="icon">
                    <use xlink:href="#icon-arrow" />
                </svg>
            </div>
        </div>
    </a>
</div>


